{
  "name": "3d-tetris",
  "repository": "3DTetris",
  "version": "0.1.0",
  "title": "3D Tetris",
  "description": "A 3D version of the classic game Tetris, with new 3D pieces in addition to the original 2D ones. Try to clear entire planes to get points!",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "COS 426",
    "ivy league",
    "Tetris",
    "3D",
    "computer graphics",
    "game",
    "video game"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path \"\"",
    "prebuild": "rimraf ./build && mkdirp ./build",
    "build": "npm run prebuild && webpack --mode production --progress --output-public-path \"\"",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production --progress && gh-pages -d build"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "three": "^0.128.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
